      <category term="算法" scheme="https://lucifer.ren/blog/tags/%E7%AE%97%E6%B3%95/"/>
    
      <category term="逆向思维" scheme="https://lucifer.ren/blog/tags/%E9%80%86%E5%90%91%E6%80%9D%E7%BB%B4/"/>
    
  </entry>
  
